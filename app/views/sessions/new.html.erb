<h1>Sign in</h1>
<%= form_with scope: :session, url: signin_path do |form| %>
    <div>
        <%= form.label "Username: " %>
        <%= form.text_field :username %>
    </div>
    <div>
        <%= form.label "Password: " %>
        <%= form.text_field :password %>
    </div>
    <div>
        <%= form.submit "Sign in.", data: { turbo: false } %>
    </div>
<% end %>